# gui browser if $DISPLAY is set
text/html; chromium %s; test=test -n "$DISPLAY"; nametemplate=%s.html      
# text browser otherwise
text/html; w3m -v -F -T text/html %s; nametemplate=%s.html; needsterminal  
# autoview
text/html; elinks --dump %s ; nametemplate=%s.html; copiousoutput          

text/plain;    less %s
text/calendar; cal2task %s

image/*; feh %s

application/vnd.ms-excel;      loffice %s
application/vnd.ms-powerpoint; loffice %s
application/msword;            antiword %s | less

application/pdf;               llpp %s

application/x-zip-compressed;        aunpack -l %s; copiousoutput

