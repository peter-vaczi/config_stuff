# gui browser if $DISPLAY is set
text/html; firefox %s; test=test -n "$DISPLAY"; nametemplate=%s.html      
# text browser otherwise
# text/html; w3m -I %{charset} -T text/html -dump; copiousoutput
text/html; elinks -dump -dump-charset %{charset}; copiousoutput

text/plain;    less %s
text/calendar; cal2task %s

image/*; feh %s

application/vnd.ms-excel;                                                  ~/.mutt/bgrun loffice %s
application/vnd.ms-powerpoint;                                             ~/.mutt/bgrun loffice %s
application/vnd.openxmlformats-officedocument.presentationml.presentation; ~/.mutt/bgrun loffice %s
application/vnd.openxmlformats-officedocument.spreadsheetml.sheet;         ~/.mutt/bgrun loffice %s
application/vnd.openxmlformats-officedocument.wordprocessingml.document;   ~/.mutt/bgrun loffice %s
application/msword;            antiword %s | less

application/pdf;               llpp %s

application/x-zip-compressed;        aunpack -l %s; copiousoutput

