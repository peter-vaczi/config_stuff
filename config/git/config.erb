[user]
	email = <%= `pass show nsn/email`.strip %>
	name  = Peter Vaczi
[push]
	default = upstream
[fetch]
  prune = true
[color]
  ui =  auto
  interactive = auto
[diff]
  compactionHeuristic = true
[pager]
  log  = diff-so-fancy | less
  show = diff-so-fancy | less
  diff = diff-so-fancy | less
[interactive]
  diffFilter = diff-so-fancy
[submodule]
  fetchJobs = 4
