# freshshell.com
#
# Examples:
#
#   fresh twe4ked/dotfiles shell/functions.sh
#   fresh jasoncodes/dotfiles aliases/rails.sh
#   fresh jasoncodes/dotfiles config/tmux.conf --file
#
# See http://freshshell.com/readme for documentation.

fresh freshshell/fresh bin/fresh --bin

# i3
fresh config/i3/config               --file=~/.i3/config
fresh config/i3/i3status-laptop.sh   --file=~/.i3/i3status-laptop.sh
fresh config/i3/i3status-laptop.conf --file=~/.i3/i3status-laptop.conf

# emacs
fresh config/emacs --file

# git
fresh config/git/config  --file=~/.gitconfig --marker
fresh config/git/aliases --file=~/.gitconfig --marker


